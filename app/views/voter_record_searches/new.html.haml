.voter-record-search
  %h3= t('lookup.title')
  %p= t('lookup.intro')

  = form_for @user, url: voter_record_searches_path, method: :post do |f|
    = render 'basic_user_fields', locals: {f: f}




    .buttons
      = link_to t('buttons.back'), :back, class: [:button, :back]
      = f.submit t('buttons.next'), class: :next
